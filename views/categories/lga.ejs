<%- layout('layouts/boilerplate') -%>
<h1 class="mb-3">List Of LGA In Delta State</h1>
<table class="table table-dark table-striped">
    <thead>
      <tr>
        <th scope="col">LGA</th>
        <th scope="col">Name</th>
        <th scope="col">Description</th>
      </tr>
    </thead>
    <tbody>
      <% for(let lga of lgas){ %>
        <tr>
            <th scope="row"><%= lga.uniqueid %> </th>
            <td><%= lga.lga_name %> </td>
            <td><%= lga.lga_description %> </td>
          </tr>
        <% } %>  
    </tbody>
  </table>